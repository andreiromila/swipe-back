{
  "manifest_version": 3,
  "name": "swipe-back",
  "version": "0.1.0",
  "description": "A browser extension that emulates swipe back in Mac.",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "run_at": "document_end",
      "js": ["swipe-back.js"],
      "css": ["swipe-back.css"]
    }
  ]
}
